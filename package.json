{
  "name": "ember-best-language",
  "version": "1.0.7",
  "description": "A FastBoot-enabled addon to detect the best language for your user.",
  "keywords": [
    "ember-addon",
    "language",
    "best",
    "locale",
    "fastboot"
  ],
  "license": "BSD-3-Clause",
  "author": "Charles Demers <cdemers@mirego.com>",
  "contributors": [
    "Pascal Boutin <pboutin@mirego.com>"
  ],
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mirego/ember-best-language",
  "scripts": {
    "build": "ember build",
    "start": "ember server",
    "test": "ember test",
    "test:all": "ember try:each",
    "prettier": "prettier --write '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "prettier-check": "prettier -l '{addon,app,tests,config}/**/*.{ts,js}' ember-cli-build.js testem.js",
    "lint": "node_modules/.bin/eslint '{addon,app,config,tests}/**/*.js' './index.js' ember-cli-build.js testem.js",
    "lint-typescript": "tslint '{addon,app,config,config,tests}/**/*.ts'",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.8.0",
    "ember-cli-typescript": "^2.0.2"
  },
  "devDependencies": {
    "@ember/optional-features": "0.7.0",
    "@types/chai": "4.1.7",
    "@types/ember": "3.1.0",
    "@types/ember-mocha": "0.14.6",
    "@types/ember-test-helpers": "1.0.5",
    "@types/ember-testing-helpers": "0.0.3",
    "@types/ember__test-helpers": "0.7.8",
    "@types/mocha": "5.2.6",
    "@types/rsvp": "4.0.2",
    "babel-eslint": "^10.0.2",
    "broccoli-asset-rev": "3.0.0",
    "chai-dom": "^1.0.0",
    "ember-cli": "3.10.1",
    "ember-cli-chai": "^0.5.0",
    "ember-cli-dependency-checker": "3.2.0",
    "ember-cli-fastboot": "2.1.1",
    "ember-cli-htmlbars": "3.1.0",
    "ember-cli-htmlbars-inline-precompile": "2.1.0",
    "ember-cli-inject-live-reload": "2.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-typescript-blueprints": "2.0.0",
    "ember-cli-uglify": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-load-initializers": "2.0.0",
    "ember-mocha": "0.15.1",
    "ember-native-dom-helpers": "0.6.2",
    "ember-resolver": "5.1.3",
    "ember-source": "3.10.1",
    "ember-source-channel-url": "2.0.1",
    "ember-try": "1.2.1",
    "eslint": "6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-ember": "^6.7.1",
    "eslint-plugin-mirego": "0.0.1",
    "loader.js": "4.7.0",
    "prettier": "^1.18.2",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*",
    "yarn": ">= 1.16.0"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
